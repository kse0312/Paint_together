{
  "name": "retrofit",
  "version": "0.2.11",
  "description": "Dynamically extend classes",
  "main": "dist/retrofit.js",
  "scripts": {
    "compile": "webpack",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "start": "webpack --watch",
    "test": "ava test.js"
  },
  "ava": {
    "babel": {
      "ignore": "dist/*",
      "presets": [
        "es2015"
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/retrofit.git"
  },
  "author": "Winton Welsh <winton@cyclo.ps>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/invrs/retrofit/issues"
  },
  "homepage": "https://github.com/invrs/retrofit#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "babel-runtime": "*"
  }
}
